[core]
	autocrlf = input
[branch]
	autosetuprebase = always
[alias]
	ci = commit
	st = status
	co = checkout
	oneline = log --pretty=oneline
	br = branch
	la = log --pretty=\"format:%ad %h (%an): %s\" --date=short
	brgr = log --oneline --decorate --graph --all
